<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="logic.users.example.enity.Movies" table="Movies" catalog="u401146528_EX">

        <id name="id" type="java.lang.Integer">
            <column name="MovieID" />
            <generator class="identity" />
        </id>

        <property name="movies" type="string">
            <column name="Movies" length="225" not-null="true" />
        </property>

        <property name="directorKey" type="java.lang.Integer">
        <column name="DirectorKey" not-null="true"  />
        </property>

        <property name="studioKey" type="java.lang.Integer">
        <column name="StudioKey" not-null="true"  />
        </property>

        <property name="runningTime" type="java.lang.Integer">
        <column name="RunningTime" not-null="true"  />
        </property>

        <property name="budget" type="java.lang.Integer">
            <column name="Budget" not-null="true"  />
        </property>

        <property name="boxOffice" type="java.lang.Integer">
            <column name="BoxOffice" not-null="true"  />
        </property>

        <property name="countryKey" type="java.lang.Integer">
            <column name="CountryKey" not-null="true"  />
        </property>

        <property name="languageKey" type="java.lang.Integer">
            <column name="LanguageKey" not-null="true"  />
        </property>

        <property name="year" type="java.lang.Integer">
            <column name="Year" not-null="true"  />
        </property>

    </class>
</hibernate-mapping>